<h2 class='title'>List of your projects</h2>
<div class="columns is-multiline">
  <% @projects.order(created_at: :desc).each do |project|%>
    <%= render partial: 'partials/show_project',  locals: { project: project}%>
  <%end%>
</div>
<%=link_to 'Create new project', new_account_project_path, class: 'button is-one-quarter'%>
<%=link_to 'Dashboard', root_path, class: 'button'  %>
